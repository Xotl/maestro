<!DOCTYPE html>
<html>
<head>
  <title>My Developer Kit</title>
<!--SCRIPTS-->
<script src="/linker/js/socket.io.js"></script>
<script src="/linker/js/sails.io.js"></script>
<script src="/linker/js/app.js"></script>
<script src="/linker/js/jquery-1.9.0.js"></script>
<script src="/linker/js/bootstrap.js"></script>
<script src="/linker/js/bootstrap.min.js"></script>
<!--SCRIPTS END-->
<!--STYLES-->
<link rel="stylesheet" href="/linker/styles/bootstrap-theme.css">
<link rel="stylesheet" href="/linker/styles/bootstrap.css">
<!--STYLES END-->
  <meta http-equiv="X-UA-Compatible" content="chrome=1">
<script>
		$(document).ready(function(){
			$(".main-menu ul li").click(function(){
				$('li.active').removeClass('active');
				$(this).addClass('active');
			});
		});
</script>
</head>
<body style="display:block;">
<section id="wrapper">
	<section id="header">
		<header>

    		<section class="header container-fluid">
    			<section class="header-wrapper">
			        <section class="section-primary">
			            <div class="app-title">
			                <a class="hp-logo" onclick="loadHome();" style="cursor: pointer;">
									<span aria-hidden="true">
										<img src="../images/devkitlogo-small.png" style="padding-bottom: 10px;">
										<span class="flashy"></span>
									</span>
			                    <h1 class="heading fs4">My Developer Kit</h1>
			                </a>
			            </div>
			        </section>

				    <section class="section-right">
			            <div class="user-panel">
                              <button id="reg-button" class="login-button" onclick="register();">Register</button>
							<button class="login-button" onclick="goLogin(this);">Login</button>
			            </div>
			        </section>
			        <section class="main-menu">
							<ul class="nav">
								<li class="divider-vertical"></li>
								<li class="active"><a onclick="loadHome();">Home</a></li>
								<li class="divider-vertical"></li>
								<li><a onclick="loadDoc();">Documentation</a></li>
								<li class="divider-vertical"></li>
                                <li class="cons" style="display: none;"><a onclick="loadConsole();">Console</a></li>
                                <li class="cons divider-vertical"  style="display: none;"></li>
                                <li class="admin" style="display: none;"><a onclick="loadAdmin();">Admin</a></li>
								<li class="admin divider-vertical"  style="display: none;"></li>
							</ul>
						<section style="clear: both;"></section>
			        </section>
		        </section>
        	<div class="plane-shadow bottom"></div>
    		</section>
		</header>
	</section>
<article id="article-wrapper">
	<%- body %>
</article>
</section>
</body>
</html>